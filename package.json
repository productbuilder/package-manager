{
  "name": "package-manager",
  "version": "1.2.5",
  "platform": "win x64",
  "main": "build/main.js",
  "license": "MIT",
  "company": "ProductBuilder",
  "productName": "Package Manager",
  "description": "Package Manager for ProductBuilder Configurations",
  "author": "Jorne Jongsma",
  "repository": {
    "type": "git",
    "url": "https://github.com/JorneJongsma/packagemanager-dev.git"
  },
  "scripts": {
    "start:render": "webpack serve --config-name renderConfig --mode development",
    "start:main": "webpack --config-name mainConfig --mode development --watch",
    "start:electron": "nodemon --exec electron build/main.js APP_DEV",
    "start": "npm-run-all -l -p start:*",
    "build": "webpack --mode production",
    "dist": "yarn build && electron-builder build --win --publish never",
    "deploy": "yarn build && electron-builder build --win --publish always",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}"
  },
  "dependencies": {
    "electron-updater": "^4.3.5",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "css-loader": "^5.0.1",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "yarn-run-all": "^3.1.1"
  }
}
